config_version=5

[application]

config/name="Mario World Mod Manager"
run/main_scene="res://main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="SuperMarioWorldRemastered"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=270
window/size/viewport_height=334
window/size/window_width_override=540
window/size/window_height_override=668
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
